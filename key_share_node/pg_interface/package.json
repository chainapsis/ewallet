{
  "name": "@keplr-ewallet/ks-node-pg-interface",
  "packageManager": "yarn@4.7.0",
  "type": "module",
  "main": "./src/index.ts",
  "scripts": {
    "migrate": "MIGRATE_MODE=all COMMITTEE_COUNT=2 tsx ./scripts/migrate/index.ts",
    "migrate_1": "MIGRATE_MODE=one COMMITTEE_ID=1 tsx ./scripts/migrate/index.ts",
    "migrate_2": "MIGRATE_MODE=one COMMITTEE_ID=2 tsx ./scripts/migrate/index.ts",
    "db_backup": "tsx ./scripts/db_backup.ts",
    "db_restore": "tsx ./scripts/db_restore.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@keplr-ewallet/bytes": "workspace:*",
    "@keplr-ewallet/key-share-node-interface": "workspace:*",
    "@keplr-ewallet/stdlib-js": "workspace:*",
    "pg": "*",
    "pgsql-parser": "^17.7.8",
    "uuid": "*"
  },
  "devDependencies": {
    "@types/jest": "*",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "jest": "*",
    "ts-jest": "*",
    "tsx": "*",
    "typescript": "*"
  }
}
